{
  "name": "hospital-graphql-server",
  "version": "1.0.0",
  "description": "Hospital Dashboard GraphQL Server",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=unit",
    "test:integration": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern=integration"
  },
  "keywords": ["graphql", "hospital", "dashboard"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-yoga": "^5.1.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
